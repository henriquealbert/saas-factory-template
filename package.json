    "react-dom": "^19.0.0",
    "reflect-metadata": "^0.2.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "overrides": {
    "@inertiajs/core": "2.0.0",
    "strtok3": "8.0.1"
  },
  "resolutions": {
    "@inertiajs/core": "2.0.0",
    "strtok3": "8.0.1"
  },
  "pnpm": {
    "overrides": {
      "@inertiajs/core": "2.0.0",
      "strtok3": "8.0.1"
